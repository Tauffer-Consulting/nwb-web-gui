                },
                "Ophys ImagingPlane imaging_rate": {
                    "type": "string",
                    "default": "ADDME"
                },
                "Ophys ImagingPlane indicator": {
                    "type": "string",
                    "default": "ADDME"
                },
                "Ophys ImagingPlane location": {
                    "type": "string",
                    "default": "primary visual cortex - layer 2/3"
                },
                "Ophys ImagingPlane name": {
                    "type": "string",
                    "default": "imaging_plane"
                },
                "Ophys ImagingPlane optical_channel description": {
                    "type": "string",
                    "default": "2P Optical Channel"
                },
                "Ophys ImagingPlane optical_channel emission_lambda": {
                    "type": "number",
                    "default": 510.0
                },
                "Ophys ImagingPlane optical_channel name": {
                    "type": "string",
                    "default": "optical_channel"
                },
                "Ophys TwoPhotonSeries name": {
                    "type": "string",
                    "default": "raw_ophys"
                },
                "Ophys TwoPhotonSeries imaging_plane": {
                    "type": "string",
                    "default": "imaging_plane"
                },
                "Ophys ImageSegmentation name": {
                	"type": "string",
                	"default": "image_segmentation"
                },
                "Ophys ImageSegmentation plane_segmentations name": {
                	"type": "string",
                	"default": "plane_segmentation"
                },
                "Ophys ImageSegmentation plane_segmentations description": {
                	"type": "string",
                	"default": "ADDME"
                },
                "Ophys ImageSegmentation plane_segmentations imaging_plane": {
                	"type": "string",
                	"default": "imaging_plane"
                },
                "Ophys ImageSegmentation plane_segmentations reference_images": {
                	"type": "string",
                	"default": "raw_ophys"
                }
            },
            "type": "object",
            "additionalProperties": false,
            "title": "Ophys"
        }
    }
]
